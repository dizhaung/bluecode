		<managed-bean-name>hostGroup</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.Group</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>serviceGroup</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.Group</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
