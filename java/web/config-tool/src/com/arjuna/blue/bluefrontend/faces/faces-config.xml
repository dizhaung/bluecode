		<navigation-case>
			<from-action>#{accountHandler.beginWizard}</from-action>
			<from-outcome>wizard-begin</from-outcome>
			<to-view-id>/macros.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-action>#{macroHandler.addMacro}</from-action>
			<from-outcome>wizard-macro-add</from-outcome>
			<to-view-id>/commands.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-action>#{blueConfigHandler.saveResult}</from-action>
			<from-outcome>wizard-config-add</from-outcome>
			<to-view-id>/timeperiods.faces</to-view-id>
			<redirect/>
		</navigation-case>
	
		<navigation-case>
			<from-action>#{commandHandler.addCommand}</from-action>
			<from-outcome>wizard-command-add</from-outcome>
			<to-view-id>/blueconfig.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-action>#{commandHandler.addCommand}</from-action>
			<from-outcome>wizard-command</from-outcome>
			<to-view-id>/commands.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-action>#{timePeriodHandler.addTimePeriod}</from-action>
			<from-outcome>wizard-timeperiod-add</from-outcome>
			<to-view-id>/contacts.faces</to-view-id>
