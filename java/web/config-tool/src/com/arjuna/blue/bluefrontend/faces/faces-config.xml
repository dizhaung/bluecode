		<managed-bean-name>hostEscalation</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.HostEscalation</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>modifyHostEscalation</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.HostEscalation</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>serviceEscalation</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.ServiceEscalation</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>modifyServiceEscalation</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.ServiceEscalation</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>escalationHandler</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.EscalationHandler</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>hostEscalation</property-name>
			<value>#{hostEscalation}</value>
		</managed-property>
		<managed-property>
			<property-name>modifyHostEscalation</property-name>
			<value>#{modifyHostEscalation}</value>
		</managed-property>
		<managed-property>
			<property-name>serviceEscalation</property-name>
			<value>#{serviceEscalation}</value>
		</managed-property>
		<managed-property>
			<property-name>modifyServiceEscalation</property-name>
			<value>#{modifyServiceEscalation}</value>
		</managed-property>
		
	</managed-bean>
	
	<!-- Service,Host Dependency bean and DependencyHandler //-->
	<managed-bean>
		<managed-bean-name>serviceDependency</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.ServiceDependency</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>	
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>modifyServiceDependency</managed-bean-name>
