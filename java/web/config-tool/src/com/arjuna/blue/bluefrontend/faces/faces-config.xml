		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>modifyGroup</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.Group</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
