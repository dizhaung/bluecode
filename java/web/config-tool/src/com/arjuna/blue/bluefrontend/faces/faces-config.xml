			<to-view-id>/servicee.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-action>#{escalationHandler.addResult}</from-action>
			<from-outcome>wizard-serviceescalation-add</from-outcome>
			<to-view-id>/serviced.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-action>#{dependencyHandler.addResult}</from-action>
			<from-outcome>wizard-servicedependency-add</from-outcome>
			<to-view-id>/serviceexti.faces</to-view-id>
			<redirect/>
