	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>modifyHostExtInfo</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.HostExtInfo</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>extInfoHandler</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.ExtInfoHandler</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>serviceExtInfo</property-name>
			<value>#{serviceExtInfo}</value>
		</managed-property>
		<managed-property>
			<property-name>modifyServiceExtInfo</property-name>
			<value>#{modifyServiceExtInfo}</value>
		</managed-property>
		<managed-property>
			<property-name>hostExtInfo</property-name>
			<value>#{hostExtInfo}</value>
		</managed-property>
		<managed-property>
			<property-name>modifyHostExtInfo</property-name>
			<value>#{modifyHostExtInfo}</value>
		</managed-property>
	</managed-bean>
	
	
	<!-- Search handling ! //-->
	<managed-bean>
		<managed-bean-name>searchHandler</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.SearchHandler</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>accountHandler</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.AccountHandler</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>blueConfig</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.BlueConfig</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
