		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>macroHandler</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.MacroHandler</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>macro</property-name>
			<value>#{macro}</value>
		</managed-property>
	</managed-bean>
		
	
	<!-- Navigation rule for Searching //-->
	<navigation-rule>
		<navigation-case>
			<from-action>#{searchHandler.searchObjects}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/search.faces</to-view-id>
			<redirect/>
		</navigation-case>
				
		<navigation-case>
			<from-outcome>add-success</from-outcome>
			<to-view-id>/addsuccess.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>delete-success</from-outcome>
			<to-view-id>/deletesuccess.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>modify-success</from-outcome>
			<to-view-id>/modifysuccess.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>success-add-1</from-outcome>
			<to-view-id>/blueconfig2.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>next-page-1</from-outcome>
			<to-view-id>/blueconfig2.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>success-add-2</from-outcome>
			<to-view-id>/blueconfig3.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>next-page-2</from-outcome>
			<to-view-id>/blueconfig3.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>success-add-3</from-outcome>
			<to-view-id>/blueconfig4.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>next-page-3</from-outcome>
			<to-view-id>/blueconfig4.faces</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>success-add-4</from-outcome>
			<to-view-id>/addconfig.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-group</from-outcome>
			<to-view-id>/modifygroup.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-host</from-outcome>
			<to-view-id>/modifyhost.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-command</from-outcome>
			<to-view-id>/modifycommand.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-contact</from-outcome>
			<to-view-id>/modifycontact.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-timeperiod</from-outcome>
			<to-view-id>/modifytimeperiod.jsp</to-view-id>
			<redirect/>
		</navigation-case>		
		
		<navigation-case>
			<from-outcome>select-service</from-outcome>
			<to-view-id>/modifyservice.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-hostexti</from-outcome>
			<to-view-id>/modifyhostexti.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-serviceexti</from-outcome>
			<to-view-id>/modifyserviceexti.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-hostdependency</from-outcome>
			<to-view-id>/modifyhostd.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-servicedependency</from-outcome>
			<to-view-id>/modifyserviced.jsp</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-serviceescalation</from-outcome>
			<to-view-id>/modifyservicee.jsp</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>select-hostescalation</from-outcome>
			<to-view-id>/modifyhoste.jsp</to-view-id>
			<redirect />
		</navigation-case>
		
