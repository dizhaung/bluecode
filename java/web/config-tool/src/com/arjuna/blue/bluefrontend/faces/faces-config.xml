		</managed-property>
		<managed-property>
			<property-name>modifyTimePeriod</property-name>
			<value>#{modifyTimePeriod}</value>
		</managed-property>
	</managed-bean>
	
	<!-- Command bean and CommandHandler //-->
	<managed-bean>
		<managed-bean-name>command</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.Command</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>modifyCommand</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.Command</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>commandHandler</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.CommandHandler</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>command</property-name>
			<value>#{command}</value>
		</managed-property>
		<managed-property>
			<property-name>modifyCommand</property-name>
			<value>#{modifyCommand}</value>
		</managed-property>
	</managed-bean>
	
	<!-- Host Escalation Bean, ServiceEscalationBean and EscalationHandler //-->
	
