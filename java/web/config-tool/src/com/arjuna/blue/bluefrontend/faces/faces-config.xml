		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>hostDependency</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.HostDependency</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>	
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>modifyHostDependency</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.HostDependency</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>dependencyHandler</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.DependencyHandler</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>hostDependency</property-name>
			<value>#{hostDependency}</value>
		</managed-property>
		<managed-property>
			<property-name>modifyHostDependency</property-name>
			<value>#{modifyHostDependency}</value>
		</managed-property>
		<managed-property>
			<property-name>serviceDependency</property-name>
			<value>#{serviceDependency}</value>
		</managed-property>
		<managed-property>
			<property-name>modifyServiceDependency</property-name>
			<value>#{modifyServiceDependency}</value>
		</managed-property>
	</managed-bean>
	
	<!-- ServiceExtInfo, HostExtInfo bean and ExtInfoHandler //-->
	<managed-bean>
		<managed-bean-name>serviceExtInfo</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.ServiceExtInfo</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>modifyServiceExtInfo</managed-bean-name>
		<managed-bean-class>com.arjuna.blue.bluefrontend.faces.ServiceExtInfo</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
